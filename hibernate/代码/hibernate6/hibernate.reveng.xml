<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="STU" schema="SYSTEM">
		<primary-key>
			<key-column name="SID" type="integer" />
		</primary-key>
	</table>
	<table name="SC" schema="SYSTEM">
		<primary-key>
			<key-column name="SCID" type="integer" />
		</primary-key>
		<column name="SID" type="integer"></column>
		<column name="SCORE" type="integer"></column>
	</table>
</hibernate-reverse-engineering>